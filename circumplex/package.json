{
  "name": "@kaliatech/circumplex",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./src/*": "./src/*",
    ".": {
      "types": "./dist/Circumplex.d.ts",
      "import": "./dist/circumplex.es.js",
      "require": "./dist/circumplex.umd.js"
    }
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write . --ignore-path ../.prettierignore --cache-location ../node_modules/.cache/circumplex/prettier",
    "test": "vitest ."
  },
  "dependencies": {
    "colorjs.io": "^0.5.2",
    "pixi.js": "^8.8.1"
  },
  "devDependencies": {
    "@eslint/js": "*",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8"
  }
}
